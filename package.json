{
    "name": "pluto",
    "version": "0.2.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "pages:build": "NODE_OPTIONS=--no-deprecation npx opennextjs-cloudflare build",
